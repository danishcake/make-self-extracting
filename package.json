{
  "name": "make-self-extracting",
  "version": "1.0.0",
  "description": "Library for creating self extracting shell scripts",
  "homepage": "https://github.com/danishcake/make-self-extracting",
  "main": "dist/libshelf.js",
  "typings": "dist/libshelf.d.ts",
  "files": [
    "dist/**/*",
    "examples/**/*"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "lint": "eslint src/**/*.ts --quiet --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsc",
    "prepare": "tsc",
    "test": "jest"
  },
  "keywords": [
    "self",
    "extracting",
    "shell",
    "tar",
    "shellscript",
    "self-extracting"
  ],
  "author": "Edward Woolhouse",
  "license": "MIT",
  "dependencies": {
    "archiver": "^5.3.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/archiver": "^5.3.1",
    "@types/gunzip-maybe": "^1.4.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/tar-stream": "^2.2.2",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gunzip-maybe": "^1.4.2",
    "jest": "^28.1.0",
    "jest-extended": "^2.0.0",
    "prettier": "^2.6.2",
    "tar-stream": "^2.2.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  }
}
